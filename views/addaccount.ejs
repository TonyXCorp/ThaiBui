<%- include('header')%>
<div class="main" style="padding:0" id="addAccount">
    <div class="sub-header">Add Account</div>
    <div class="display"></div>
    <div class="container-fluid">
        <form action="/account-mutiadd" method="POST">
            <label for="company">Gõ MutiAccount (user | password)</label>
            <div class="row">
                <textarea class="form-control search-bar" name="input_mutiuser" id="ac_email" type="text" placeholder="Nhập user cần thêm" rows="8"></textarea>
            </div>
            <div class="row">
                    <input class="btn btn-primary" type="submit" id="btn-sub" name="submit" value="Nhận URL auth" style="margin-left: 47%">
                    <span style="margin-left: 48%" class="success-msg"><%= error %></span>
            </div>
        </form>
    </div>
</div>
</body>
<script src="https://cdn.jsdelivr.net/npm/vue@2.6.12/dist/vue.js"></script>
<script src="js/addAccount.js"></script>
<link rel="stylesheet" href="css/listVideo.css">
</html>